{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "born05/craft-assetusage": "^4.1.3",
    "craftcms/ckeditor": "^4.7.0",
    "craftcms/cms": "^5.0.0",
    "nicholashamilton/craft-preview-mate": "^4.1.0",
    "nystudio107/craft-vite": "^5.0",
    "spicyweb/craft-embedded-assets": "^5.4.3",
    "vandres/craft-matrix-extended": "^v4.2.1",
    "verbb/field-manager": "^4.0.3",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  }
}
